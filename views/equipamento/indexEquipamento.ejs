<%- include("../layouts/header", {title: 'EQUIPAMENTOS' }) %>

  <%- include("../layouts/menu") %>

    <div class="container_1">
      <h1>Equipamentos</h1>
      <div class="coluna_1">
        <div class="coluna_conteudo">
          <a href="/">Voltar</a>
        </div>
        <div class="coluna_conteudo">
          <a href="/">Voltar</a>
        </div>
        <div class="coluna_conteudo">
          <a href="/">Voltar</a>
        </div>
      </div>
      <div class="container_tabela">
        <table>
          <thead>
            <tr class="item">
              <th>ID</th>
              <th>Tag Equipamento</th>
            </tr>
          </thead>
          <tbody>
            <% equipamento.forEach(equipamento=> { %>
              <tr class="item">
                <td>
                  <%= equipamento.id_equipamento %>
                </td>
                <td>
                  <%= equipamento.tag_equipamento %>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>
    </div>

